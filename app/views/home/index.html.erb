<h1>Sistema de Galpões e Estoque</h1>
<h2>Galpões e Estoques</h2>
<%=notice%>
<%=link_to 'Cadastrar Galpão', new_warehouse_path%>
<%if @warehouses.empty?%>
    <p>Não existem galpões cadastrados</p>
<%end%>
<%@warehouses.each do |w| %>
  <%=link_to(w.name, warehouse_path(w.id))%></br>
  Codigo: <%=w.code%></br>
  Cidade: <%=w.city%></br>
  <%=w.area%> m2 </br></br>
<% end %>